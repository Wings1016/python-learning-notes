#!/usr/bin/env python3
# -*- coding: utf-8 -*-

import random, time, queue
from multiprocessing.managers import BaseManager

task_queue = queue.Queue()
result_queue = queue.Queue()

class QueueManager(BaseManager):
    pass

QueueManager.register('get',callable=lambda:task_queue)
QueueManager.register('result',callable=lambda:result_queue)

manager = QueueManager(address=('',5000),authkey = b'123')
manager.start()

task = manager.get()
result = manager.result()

for i in range(10):
    n = random.randint(0, 10000)
    print('Put task %d...' % n)
    task.put(n)

print('尝试读取结果：')

for i in range(10):
    r = result.get(timeout=10)
    print('结果是：%s' % r)

manager.shutdown()
print('master exit.')
